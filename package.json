{
  "name": "quantumviz",
  "version": "0.0.1",
  "description": "Quantumviz Elements",
  "main": "dist/quantumviz.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
