{
  "name": "@senx/warpview",
  "version": "2.0.24",
  "keywords": [
    "web",
    "dataviz",
    "web components",
    "time series",
    "warp10",
    "Warp 10",
    "WarpScript",
    "Geo Times Series",
    "Times Series"
  ],
  "description": "WarpView Elements",
  "repository": {
    "type": "git",
    "url": "https://github.com/senx/warpview.git"
  },
  "author": "SenX <contact@senx.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/senx/warpview/issues"
  },
  "homepage": "https://www.warp10.io",
  "main": "dist/bundles/warpview-ng.umd.js",
  "module": "dist/fesm5/warpview-ng.js",
  "es2015": "dist/fesm2015/warpview-ng.js",
  "esm5": "dist/esm5/warpview-ng.js",
  "esm2015": "dist/esm2015/warpview-ng.js",
  "fesm5": "dist/fesm5/warpview-ng.js",
  "fesm2015": "dist/fesm2015/warpview-ng.js",
  "typings": "dist/warpview-ng.d.ts",
  "metadata": "dist/warpview-ng.metadata.json",
  "files": [
    "./dist/warpview"
  ],
  "scripts": {
    "ng": "ng",
    "sass": "./node_modules/node-sass/bin/node-sass",
    "dev": "node --max_old_space_size=2048 --optimize_for_size --stack_size=2048 ./node_modules/@angular/cli/bin/ng serve",
    "build": "yarn clean && yarn build:lib && yarn build:elements",
    "build:demo": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gen": "cd projects/warpview-ng/src/lib/elements && ng g component --module=warp-view-angular.module --entryComponent=true --skipTests=true --style=scss --viewEncapsulation=ShadowDom --export=true",
    "gh": "ng build --prod --base-href /warpview/ && npx ngh --dir=dist/warpview-elements --repo=git@github.com:senx/warpview.git",
    "build:elements": "ng build --prod --single-bundle=true --output-hashing=none  --project elements && node scripts/build.js",
    "build:lib2": "ngc -p src/tsconfig.elements-ng.json &&  ./node_modules/node-sass/bin/node-sass ./src/styles.scss ./dist/warpview-ng/style.css",
    "build:gh": "ng build --prod --output-hashing=none  --project warpview",
    "clean": "rm -fr ./elements && rm -fr ./dist",
    "prepublishOnly": "yarn clean && yarn build",
    "build:lib": "ng build  --project=warpview-ng",
    "nvm": "nvm install 10.18.0",
    "build:pack": "(cd dist/warpview && yarn pack)",
    "build:publish": "yarn version $1 && (cd project/warpview-ng && yarn version $1 )  && yarn clean && yarn build && (cd dist/warpview && yarn publish)"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.1.5",
    "@angular/common": "~10.1.5",
    "@angular/compiler": "~10.1.5",
    "@angular/core": "~10.1.5",
    "@angular/elements": "^10.1.5",
    "@angular/forms": "~10.1.5",
    "@angular/platform-browser": "~10.1.5",
    "@angular/platform-browser-dynamic": "~10.1.5",
    "@angular/router": "~10.1.5",
    "@webcomponents/custom-elements": "^1.4.2",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "angular-resize-event": "^2.0.0",
    "bootstrap": "^4.5.2",
    "canvas-gauges": "^2.1.7",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "d3": "^5.15.0",
    "deep-equal": "^2.0.4",
    "document-register-element": "1.14.10",
    "event-drops": "^1.3.0",
    "fitty": "^2.3.2",
    "jquery": "^3.5.1",
    "leaflet": "1.7.1",
    "leaflet-ant-path": "^1.3.0",
    "leaflet.animatedmarker": "^1.0.0",
    "leaflet.glify": "^3.0.0",
    "leaflet.heat": "^0.2.0",
    "leaflet.markercluster": "^1.4.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "nouislider": "^14.6.2",
    "plotly.js": "^1.57.1",
    "plotly.js-dist": "^1.57.1",
    "popper": "^1.0.1",
    "prismjs": "^1.21.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.2",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.6",
    "@angular-devkit/build-ng-packagr": "~0.1001.6",
    "@angular/cli": "~10.1.6",
    "@angular/compiler-cli": "~10.1.5",
    "@angular/language-service": "~10.1.5",
    "@types/canvas-gauges": "^2.1.1",
    "@types/d3": "^5.7.2",
    "@types/deep-equal": "^1.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^21.1.1",
    "@types/leaflet": "^1.5.12",
    "@types/leaflet.heat": "^0.2.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.11.1",
    "@types/plotly.js": "^1.44.24",
    "@webcomponents/custom-elements": "^1.4.2",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.1.2",
    "concat": "^1.0.3",
    "fs-extra": "^8.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "ngx-build-plus": "10.1.1",
    "node-sass": "^4.14.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typedoc": "^0.15.0",
    "typescript": "~4.0.3"
  }
}
