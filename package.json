{
  "name": "@senx/warpview",
  "module": "dist/esm/index.js",
  "version": "1.0.28",
  "keywords": [
    "web",
    "dataviz",
    "web components",
    "time series",
    "warp10",
    "Warp 10",
    "WarpScript",
    "Geo Times Series",
    "Times Series"
  ],
  "description": "WarpView Elements",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/senx/warpview.git"
  },
  "author": "SenX <contact@senx.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/senx/warpview/issues"
  },
  "homepage": "https://www.warp10.io",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --prod",
    "dev": "stencil build --dev --watch --serve",
    "devff": "stencil build --dev --watch --serve --es5",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "start": "yarn run dev "
  },
  "dependencies": {
    "@code-dimension/stencil-components": "0.0.0-beta.1",
    "chart.js": "^2.7.2",
    "clipboard": "^2.0.1",
    "d3": "^5.7.0",
    "deep-equal": "^1.0.1",
    "dygraphs": "^2.1.0",
    "leaflet": "1.3.3",
    "leaflet.heat": "^0.2.0",
    "leaflet.markercluster": "^1.3.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "prismjs": "^1.14.0",
    "rebuild": "^0.1.2",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@stencil/core": "~0.16.2",
    "@stencil/sass": "^0.1.1",
    "@types/chartjs": "0.0.31",
    "@types/d3": "^5.0.1",
    "@types/dygraphs": "^1.1.8",
    "@types/jest": "^22.2.3",
    "@types/leaflet": "^1.2.8",
    "@types/leaflet.heat": "^0.2.0",
    "@types/moment-timezone": "^0.5.9",
    "@types/prismjs": "^1.9.0",
    "jest": "^22.4.3",
    "node-gyp": "^3.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
