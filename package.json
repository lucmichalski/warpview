{
  "name": "@senx/warpview",
  "module": "dist/esm/index.js",
  "version": "1.0.49",
  "keywords": [
    "web",
    "dataviz",
    "web components",
    "time series",
    "warp10",
    "Warp 10",
    "WarpScript",
    "Geo Times Series",
    "Times Series"
  ],
  "description": "WarpView Elements",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/senx/warpview.git"
  },
  "author": "SenX <contact@senx.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/senx/warpview/issues"
  },
  "homepage": "https://www.warp10.io",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --prod",
    "dev": "stencil build --dev --watch --serve",
    "devff": "stencil build --dev --watch --serve --es5",
    "devprod": "stencil build --prod --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "start": "yarn run dev "
  },
  "dependencies": {
    "@code-dimension/stencil-components": "0.0.0-beta.1",
    "chart.js": "^2.7.3",
    "clipboard": "^2.0.4",
    "d3": "^5.7.0",
    "deep-equal": "^1.0.1",
    "dygraphs": "^2.1.0",
    "leaflet": "1.4.0",
    "leaflet.heat": "^0.2.0",
    "leaflet.markercluster": "^1.4.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "rebuild": "^0.1.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@stencil/core": "~0.17.2",
    "@stencil/sass": "^0.1.1",
    "@types/chartjs": "0.0.31",
    "@types/d3": "^5.7.0",
    "@types/dygraphs": "^1.1.8",
    "@types/jest": "^23.3.13",
    "@types/leaflet": "^1.2.14",
    "@types/leaflet.heat": "^0.2.0",
    "@types/moment-timezone": "^0.5.10",
    "@types/prismjs": "^1.9.0",
    "@vismalietuva/tslint-formatter-html": "^1.0.1",
    "jest": "^23.6.0",
    "node-gyp": "^3.8.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-stencil": "^1.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
